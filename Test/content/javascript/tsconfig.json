{
    "compilerOptions": {
        "checkJs": true,
        "baseUrl": "./scripts/",
        "target": "es5",
        "module": "none",
        "lib": ["es5"],
        "types": [
            "none"
        ],
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
